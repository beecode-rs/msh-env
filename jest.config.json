{
  "collectCoverage": true,
  "collectCoverageFrom": ["src/**/*.ts", "!src/**/*.{contract,d}.ts"],
  "moduleNameMapper": {
    "^src/(.*)$": "<rootDir>/src/$1"
  },
  "preset": "ts-jest",
  "roots": ["./src"],
  "setupFilesAfterEnv": ["jest-extended/all"],
  "testEnvironment": "node",
  "testPathIgnorePatterns": ["/node_modules/", "/test/"]
}
